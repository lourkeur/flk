xkb_symbols "custom_xbows" {

    include "custom"

    // Repurpose left shift as a duplicate backslash
    key <LFSH> { [ backslash, bar ] };

    // X-Bows Nature&Knight don't support numlock
    key  <KP0> {	[  KP_0	]	};
    key  <KP1> {	[  KP_1	]	};
    key  <KP2> {	[  KP_2	]	};
    key  <KP3> {	[  KP_3	]	};
    key  <KP4> {	[  KP_4	]	};
    key  <KP5> {	[  KP_5	]	};
    key  <KP6> {	[  KP_6	]	};
    key  <KP7> {	[  KP_7	]	};
    key  <KP8> {	[  KP_8	]	};
    key  <KP9> {	[  KP_9	]	};
};
